<!DOCTYPE html>
<html lang="en">

<head>
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.16/css/dataTables.bootstrap4.min.css">
</head>
<% include partials/scripts.ejs %>

    <body onload="loadAllComplaints()">
            <nav class="navbar navbar-expand-sm navbar-dark bg-primary">
                    <a class="navbar-brand" href="#">Organization</a>
                    <div class="container">
                        <div class="row">
                            <a class="btn secondary white btn-lg " href="/profile">Profile</a>
                            <a class="btn secondary white btn-lg " href="/logout">Logout</a>
                        </div>
                    </div>
                    <p style="color : white;"><%= user %></p>
            </nav>
            <!-- complaints -->
            <div class="container">
                <div id="complaint-heading">
                    <p class="h1 text-center">All Suggestions &amp; Objections</p>
                </div>
                <button><a class="remove-underline" href="/newComplaint">Add New Complaint</a></button>
                    <div class="row table-responsive-sm">
                        <table class="table table-striped table-bordered table-sm" cellspacing="0">
                            <thead class="thead-dark">
                                <tr>
                                    <th scope="col" class="th-font">Id</th>
                                    <th scope="col" class="th-font">Name</th>
                                    <th scope="col" class="th-font">Type</th>
                                    <th scope="col" class="th-font">DateTime</th>
                                    <th scope="col" class="th-font">Location</th>
                                    <th scope="col" class="th-font">Relevant ParaClause</th>
                                    <th scope="col" class="th-font">Description</th>
                                    <th scope="col" class="th-font">Status</th>
                                    <th scope="col" class="th-font">Preview</th>
                                </tr>
                            </thead>
                            <tbody id="ctable">
                            </tbody>
                        </table>
                    </div>
            </div>
            <!-- complaints end -->
            <!-- <script src="../controllers/complaintsController.js"></script> -->

    </body>

</html>